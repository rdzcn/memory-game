generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Game {
  id           String    @id @default(auto()) @map("_id") @db.ObjectId
  gameId       String    @unique
  title        String
  player1Name  String
  player2Name  String
  player1Score Int
  player2Score Int
  winner       String
  cardCount    Int
  createdAt    Float
  updatedAt    Float
  finishedAt   Float
}
