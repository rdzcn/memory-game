{
	"name": "memory-game",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"client": "pnpm --filter @memory-game/client run dev",
		"backend": "pnpm --filter @memory-game/backend run dev",
		"format": "pnpm biome write",
		"check-types": "tsc --noEmit",
		"build": "pnpm -r build --filter=!backend",
		"postbuild": "cd apps/backend && pnpm build",
		"dev": "pnpm -r dev",
		"lint": "pnpm -r lint",
		"db:generate": "pnpm --filter @memory-game/database run db:generate",
		"db:migrate:dev": "pnpm --filter @memory-game/database run db:migrate:dev",
		"db:migrate:deploy": "pnpm --filter @memory-game/database run db:migrate:deploy",
		"db:seed": "pnpm --filter @memory-game/database run db:seed"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"biome": "^0.3.3"
	},
	"devDependencies": {
		"cross-env": "^7.0.3"
	},
	"packageManager": "pnpm@9.14.1"
}
